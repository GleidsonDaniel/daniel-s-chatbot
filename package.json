{
  "name": "daniels-chatbot",
  "version": "1.0.0",
  "private": true,
  "description": "Esta é uma demonstração simples de um chatbot, criado com React, React Native e Express",
  "scripts": {
    "server": "cd chatbot-api && npm start",
    "dev-server": "cd chatbot-api && npm run dev",
    "client": "cd chatbot-ui-web && npm start",
    "start": "./node_modules/.bin/concurrently \"npm run server\" \"npm run client\"",
    "dev": "./node_modules/.bin/concurrently \"npm run dev-server\" \"npm run client\""
  },
  "author": "Gleidson Daniel Jacinto da Silva",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^4.1.2"
  }
}
